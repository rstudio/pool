Type: Package
Package: pool
Title: Object Pooling
Version: 0.1.6.9000
Authors@R: c(
    person("Joe", "Cheng", , "joe@rstudio.com", role = c("aut", "cre")),
    person("Barbara", "Borges", , "barb.b.ribeiro@gmail.com", role = "aut"),
    person(, "RStudio", role = "cph")
